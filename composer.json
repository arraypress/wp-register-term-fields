{
  "name": "arraypress/wp-register-term-fields",
  "version": "1.0.0",
  "description": "Lightweight library for registering custom fields on WordPress taxonomy term screens.",
  "homepage": "https://github.com/arraypress/wp-register-term-fields/",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\RegisterTermFields\\": "src/"
    },
    "files": [
      "src/Utilities/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-term-fields/issues",
    "source": "https://github.com/arraypress/wp-register-term-fields"
  },
  "keywords": [
    "wordpress",
    "taxonomy",
    "term-fields",
    "term-meta",
    "custom-fields",
    "admin"
  ]
}
